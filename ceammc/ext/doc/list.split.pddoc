<?xml version="1.0" encoding="UTF-8"?>
<pddoc version="1.0">
    <object name="list.split">
        <title>list.split</title>
        <meta>
            <authors>
                <author>Alex Nadzharov</author>
                <author>Serge Poltavsky</author>
            </authors>
            <description>splits list into two parts, the length of first list is specified by the argument.</description>
            <license>GPL3</license>
            <library>ceammc</library>
            <category>list</category>
            <keywords>list</keywords>
            <since>0.1</since>
            <also>
                <see>list.split</see>
            </also>
        </meta>
        <arguments>
            <argument name="split" type="int">size of first list. The size of the list on the second output is the size of input list - this value</argument>
        </arguments>
        <inlets>
            <inlet>
                <xinfo on="list">Input list. </xinfo>
            </inlet>
            <inlet>
                <xinfo>New split point value.</xinfo>
            </inlet>
        </inlets>
        <outlets>
            <outlet>first sub-list of given size</outlet>
            <outlet>second sub-list</outlet>
        </outlets>
        <example>
            <pdascii>
                <![CDATA[
[1 2 3 4 5(  [3 (
|            |.
[list.split      2]
|              ^|
[prepend set]   [prepend set]
|               |
[ (             [ (
                ]]>
            </pdascii>
        </example>
    </object>
</pddoc>

